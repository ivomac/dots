{
  "$schema": "https://opencode.ai/config.json",
  "theme": "gruv",
  "model": "deepseek/deepseek-chat",
  "small_model": "deepseek/deepseek-chat",
  "autoupdate": false,
  "share": "manual",
  "snapshot": false,
  "username": "Ivo",
  "disabled_providers": [],
  "tui": {
    "scroll_speed": 2
  },
  "keybinds": {
    "leader": "tab",
    "agent_cycle": "ctrl+f",
    "agent_cycle_reverse": "none",
    "agent_list": "<leader>a",
    "app_exit": "ctrl+c",
    "command_list": "<leader>space",
    "editor_open": "<leader>e",
    "history_next": "down",
    "history_previous": "up",
    "input_clear": "none",
    "input_forward_delete": "none",
    "input_newline": "shift+enter",
    "input_paste": "ctrl+v",
    "input_submit": "enter",
    "messages_copy": "<leader>y",
    "messages_first": "none",
    "messages_half_page_down": "ctrl+j",
    "messages_half_page_up": "ctrl+k",
    "messages_last": "none",
    "messages_page_down": "none",
    "messages_page_up": "none",
    "messages_redo": "<leader>r",
    "messages_toggle_conceal": "<leader>h",
    "messages_undo": "<leader>u",
    "model_cycle_recent": "ctrl+m",
    "model_cycle_recent_reverse": "none",
    "model_list": "<leader>m",
    "session_child_cycle": "ctrl+s",
    "session_child_cycle_reverse": "none",
    "session_compact": "none",
    "session_export": "none",
    "session_interrupt": "esc",
    "session_list": "<leader>s",
    "session_new": "none",
    "session_share": "none",
    "session_timeline": "<leader>t",
    "session_unshare": "none",
    "sidebar_toggle": "none",
    "status_view": "none",
    "theme_list": "none"
  },
  "agent": {},
  "permission": {
    "edit": "ask",
    "bash": "ask",
    "webfetch": "allow"
  },
  "tools": {
    "bash": true,
    "glob": true,
    "grep": true,
    "list": true,
    "patch": true,
    "read": true,
    "todoread": true,
    "todowrite": true,
    "webfetch": true,
    "write": true
  },
  "command": {},
  "watcher": {
    "ignore": []
  },
  "plugin": [],
  "instructions": [],
  "provider": {},
  "mcp": {},
  "formatter": {},
  "lsp": {},
  "experimental": {
    "hook": {},
    "chatMaxRetries": 3,
    "disable_paste_summary": false
  }
}
